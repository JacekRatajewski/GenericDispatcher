<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Jacek Ratajewski</Authors>
    <Description>Generic Dispatcher - Module managing saving and loading data from/to json compatible with Standard.</Description>
    <PackageProjectUrl>https://github.com/JacekRatajewski/GenericDispatcher</PackageProjectUrl>
    <PackageIconUrl>https://i.imgur.com/AhMHxa0.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/JacekRatajewski/GenericDispatcher.git</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
